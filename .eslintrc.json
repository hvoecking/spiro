{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "no-console": ["error", { "allow": ["warn", "error"] }],
      "semi": ["error", "always"],
      "quotes": ["error", "double", { "allowTemplateLiterals": true }]
    }
  }
